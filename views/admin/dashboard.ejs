Dashboard
<div class="admin-title">
    <h2>Posts</h2>
    <a href="/add-post" class="button">+ Add Post</a>
</div>

<ul class="admin-posts">
    <% data.forEach(element => { %>

    <li>
        <a href="/post/<%= element._id %>">
            <%= element.title %> &nearr;
        </a>
        <div class="admin-post-controls">
            <a href="/edit-post/<%= element._id %>" class="btn">Edit</a>
            
            <form action="/delete-post/<%= element._id %>?_method=DELETE" method="post">
                <input type="submit" value="Delete" class="btn">
            </form>
            
        </div>








    </li>



        
   <% }); %>
</ul>